<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="EventItemReminderIcon">
        <item name="android:tint">@color/dark_liver</item>
    </style>

    <style name="EventItemReminderSubtext">
        <item name="android:textColor">@color/onyx</item>
        <item name="android:textSize">14sp</item>
        <item name="fontFamily">@font/blogger_sans_bold</item>
    </style>

    <style name="ReminderBlockTitleTextView">
        <item name="android:fontFamily">@font/blogger_sans_bold</item>
        <item name="android:textColor">@color/gray_chateau</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="ReminderBlockBigTitleTextView">
        <item name="android:fontFamily">@font/blogger_sans_bold</item>
        <item name="android:textColor">@color/onyx</item>
        <item name="android:textSize">20sp</item>
    </style>

    <style name="DialogTitle">
        <item name="android:fontFamily">@font/blogger_sans_bold</item>
        <item name="android:textColor">@color/metallic_seaweed</item>
        <item name="android:textSize">20sp</item>
    </style>

    <style name="ReminderCardStyle">
        <item name="cardCornerRadius">10dp</item>
        <item name="cardElevation">5dp</item>
        <item name="cardBackgroundColor">@color/white</item>
    </style>

    <style name="ViewReminderCardStyle">
        <item name="cardCornerRadius">10dp</item>
        <item name="cardElevation">0dp</item>
        <item name="cardBackgroundColor">@color/white</item>
    </style>

    <style name="ReminderOpenMenuIcon">
        <item name="android:layout_width">12dp</item>
        <item name="android:layout_height">12dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:rotation">90</item>
        <item name="android:src">@drawable/ic_open_menu</item>
        <item name="android:tint">@color/gray_chateau</item>
    </style>

    <style name="ButtonIconColor">
        <item name="android:layout_gravity">center</item>
        <item name="android:tint">@color/white</item>
    </style>

    <style name="ReminderBlockContentTextView">
        <item name="android:fontFamily">@font/blogger_sans_bold</item>
        <item name="android:textColor">@color/onyx</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="ReminderBigBlockTitleTextView">
        <item name="android:fontFamily">@font/blogger_sans_bold</item>
        <item name="android:textColor">@color/onyx</item>
        <item name="android:textSize">26sp</item>
    </style>

    <style name="CircleInText">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:scaleType">fitCenter</item>
        <item name="android:padding">4dp</item>
        <item name="android:tint">@color/dark_liver</item>
    </style>

    <style name="CircleDateTimeDivider">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:scaleType">fitCenter</item>
        <item name="android:padding">4dp</item>
        <item name="android:tint">@color/onyx</item>
    </style>

    <style name="TitleEditText">
        <item name="android:fontFamily">@font/blogger_sans_bold</item>
        <item name="android:textSize">33sp</item>
        <item name="android:inputType">textCapSentences</item>
        <item name="maxLines">1</item>
        <item name="android:textColorHint">@color/gray_chateau</item>
        <item name="android:background">@color/transparent</item>
        <item name="android:textColor">@color/onyx</item>
    </style>

    <style name="CustomCalendarMonth" parent="Theme.MadDiary">
        <item name="android:textColorPrimary">@color/onyx</item>
        <item name="fontFamily">@font/blogger_sans_bold</item>
    </style>

    <style name="CustomCalendarDay" parent="TextAppearance.MaterialComponents.Caption">
        <item name="android:textColor">@color/selector_calendar_color</item>
        <item name="android:weekNumberColor">@color/gray_chateau</item>
        <item name="fontFamily">@font/blogger_sans_bold</item>
    </style>

    <style name="CustomCalendarWeek" parent="TextAppearance.MaterialComponents.Caption">
        <item name="android:textColor">@color/gray_chateau</item>
        <item name="fontFamily">@font/blogger_sans_bold</item>
    </style>

    <style name="BottomSheetHandle">
        <item name="android:background">@drawable/bottom_sheet_handle</item>
        <item name="android:backgroundTint">@color/gray_chateau</item>
        <item name="android:layout_width">60dp</item>
        <item name="android:layout_height">4dp</item>
        <item name="android:layout_marginTop">8dp</item>
    </style>

    <style name="TimePickerStyle" parent="Theme.MadDiary">
        <item name="colorControlNormal">@color/transparent</item>
        <item name="android:textColor">@color/onyx</item>
        <item name="android:fontFamily">@font/blogger_sans_bold</item>
        <item name="android:descendantFocusability">blocksDescendants</item>
        <item name="android:textSize">26sp</item>
    </style>

    <style name="TimePickerDivider">
        <item name="android:textColor">@color/onyx</item>
        <item name="fontFamily">@font/blogger_sans_bold</item>
        <item name="android:textSize">40sp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="RadioButtonStyle" parent="Widget.MaterialComponents.CompoundButton.RadioButton">
        <item name="materialThemeOverlay">@style/ThemeOverlay.App.RadioButton</item>
    </style>

    <style name="ThemeOverlay.App.RadioButton" parent="">
        <item name="colorOnSurface">@color/gray_chateau</item>
        <item name="colorSecondary">@color/metallic_seaweed</item>
        <item name="android:textColor">@color/onyx</item>
        <item name="android:fontFamily">@font/blogger_sans_bold</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="CheckBoxStyle" parent="Widget.MaterialComponents.CompoundButton.CheckBox">
        <item name="materialThemeOverlay">@style/ThemeOverlay.App.CheckBox</item>
    </style>

    <style name="ThemeOverlay.App.CheckBox" parent="">
        <item name="colorOnSurface">@color/gray_chateau</item>
        <item name="colorSecondary">@color/metallic_seaweed</item>
        <item name="android:textColor">@color/onyx</item>
        <item name="android:fontFamily">@font/blogger_sans_bold</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="CalendarCellDayNumber">
        <item name="android:fontFamily">@font/blogger_sans_bold</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/selector_calendar_color</item>
    </style>

    <style name="DialogTheme_transparent" parent="Theme.AppCompat.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>

    <style name="DialogCardStyle">
        <item name="cardBackgroundColor">@color/white</item>
        <item name="cardCornerRadius">20dp</item>
    </style>

    <style name="CalendarButtonStyle">
        <item name="android:tint">@color/onyx</item>
        <item name="android:background">@color/transparent</item>
    </style>

    <style name="ItemTitleDateStyle">
        <item name="android:fontFamily">@font/blogger_sans_bold</item>
        <item name="android:textSize">17sp</item>
        <item name="android:textColor">@color/dark_liver</item>
        <item name="android:layout_gravity">center</item>
    </style>

    <style name="CreateReminderChooseTypeTitleStyle">
        <item name="android:fontFamily">@font/blogger_sans_bold</item>
        <item name="android:textColor">@color/onyx</item>
        <item name="android:textSize">20sp</item>
    </style>

    <style name="EventCover">
        <item name="android:foreground">#00000000</item>
    </style>

    <style name="ShapeAppearanceOverlay.App.CornerSize10Percent" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">10%</item>
    </style>

    <style name="ShapeAppearanceOverlay.App.CornerSize15Percent" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">15%</item>
    </style>

    <style name="BottomSheetAttachmentTypeStyle">
        <item name="android:background">@color/white</item>
    </style>

    <style name="EventContentBackground">
        <item name="android:background">@drawable/top_corners_15</item>
        <item name="android:backgroundTint">@color/white</item>
    </style>

    <style name="BottomSheetAttachmentStyle">
        <item name="android:background">@drawable/top_corners</item>
        <item name="android:backgroundTint">@color/white</item>
    </style>

    <style name="BottomSheetDividerStyle">
        <item name="android:background">@color/cloud</item>
    </style>

    <style name="ItemCoverStyle" parent="Widget.MaterialComponents.CardView">
        <item name="strokeWidth">2dp</item>
        <item name="strokeColor">@color/onyx</item>
    </style>

    <style name="MaterialAlertDialog.App" parent="MaterialAlertDialog.MaterialComponents">
        <item name="shapeAppearance">@style/ShapeAppearance.App.MediumComponent</item>
    </style>

    <style name="MaterialAlertDialog.App.Title.Text" parent="MaterialAlertDialog.MaterialComponents.Title.Text">
        <item name="android:textColor">@color/onyx</item>
        <item name="fontFamily">@font/blogger_sans_bold</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="MaterialAlertDialog.App.Body.Text" parent="TextAppearance.MaterialComponents.Body2">
        <item name="android:textColor">@color/onyx</item>
        <item name="fontFamily">@font/blogger_sans_medium</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="Widget.App.Button" parent="Widget.MaterialComponents.Button.TextButton.Dialog">
        <item name="materialThemeOverlay">@style/ThemeOverlay.App.Button.TextButton</item>
        <item name="shapeAppearance">@style/ShapeAppearance.App.SmallComponent</item>
    </style>

    <style name="ThemeOverlay.App.Button.TextButton" parent="Widget.MaterialComponents.Button.TextButton">
        <item name="fontFamily">@font/blogger_sans_bold</item>
        <item name="android:textSize">12sp</item>
        <item name="android:letterSpacing">0</item>
    </style>

    <style name="ThemeOverlay.App.Button" parent="">
        <item name="colorPrimary">@color/metallic_seaweed</item>
    </style>

    <style name="ShapeAppearance.App.MediumComponent" parent="ShapeAppearance.MaterialComponents.MediumComponent">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">15dp</item>
    </style>

    <style name="ShapeAppearance.App.SmallComponent" parent="ShapeAppearance.MaterialComponents.SmallComponent">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">8dp</item>
    </style>

    <style name="AttachCardStyle">
        <item name="strokeWidth">1dp</item>
        <item name="strokeColor">@color/onyx</item>
        <item name="cardBackgroundColor">@color/transparent</item>
        <item name="cardElevation">0dp</item>
        <item name="cardCornerRadius">10dp</item>
    </style>

    <style name="WaveView">
        <item name="mad_waveLineColor">@color/metallic_seaweed</item>
        <item name="android:background">@color/mercury</item>
        <item name="mad_waveLineWidth">2dp</item>
    </style>

</resources>